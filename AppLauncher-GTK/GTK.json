{
  "compiler_path": "g++",
  "compiler_std": "c++20",
  "headers_path": ["../dev"],
  "sources_path": ["*.cxx"],
  "output_name": "AppLauncher.exe",
  "compiler_flags": [
    "-lLinuxFoundation",
    "-lGraphicsKit",
    "-lAnimationKit",
    "-lFoundationKit",
    "$(pkg-config --cflags --libs gtk+-3.0 cairo glib-2.0)",
    "$(pkg-config --cflags --libs gtk+-3.0)"
  ],
  "cpp_macros": ["__APPLAUNCHER__"],
  "run_after_build": false
}
